    <xs:simpleType name="ValidationEnum" id="_9089a62e-e081-4c85-82e5-c2bcf3787ca4">
        <xs:annotation>
            <xs:documentation>
                Validation enum classifies possible reasons for creating extended logging
            </xs:documentation>
            <xs:appinfo>
                <meta:metaInfo>
                    <meta:xml>
                        <meta:old>
                            <meta:name xmlns="http://vdv/ka/kvp-dl-pv/2">Validation_CODE_Type</meta:name>
                            <meta:deletedValue>
                                <meta:value>0</meta:value>
                                <meta:documentation>Keine Angabe</meta:documentation>
                                <meta:reason>Undefined values should not be used.</meta:reason>
                            </meta:deletedValue>
                            <meta:deletedValue>
                                <meta:value>7</meta:value>
                                <meta:documentation>sym. Schluessel gesperrt oder nicht vorhanden</meta:documentation>
                                <meta:reason>Relevant symmetric keys are obsolete</meta:reason>
                            </meta:deletedValue>
                            <meta:deletedValue>
                                <meta:value>8</meta:value>
                                <meta:documentation>asym. Schuessel gesperrt oder nicht vorhanden</meta:documentation>
                                <meta:reason>Relevant asymmetric keys are obsolete</meta:reason>
                            </meta:deletedValue>
                            <meta:deletedValue>
                                <meta:value>9</meta:value>
                                <meta:documentation>Status im App. Verzeichnis nicht gesetzt</meta:documentation>
                                <meta:reason>UM sets status - therefore it is obsolete</meta:reason>
                            </meta:deletedValue>
                            <meta:deletedValue>
                                <meta:value>10</meta:value>
                                <meta:documentation>Ausgabetransaktionskennung fehlt</meta:documentation>
                                <meta:reason>Obsolete in the new static entitlement structure</meta:reason>
                            </meta:deletedValue>
                        </meta:old>
                    </meta:xml>
                </meta:metaInfo>
            </xs:appinfo>
        </xs:annotation>
        <xs:restriction base="tns:ValidationCode">
            <xs:enumeration value="1">
                <xs:annotation>
                    <xs:documentation>
                        Inauthentic data
                    </xs:documentation>
                    <xs:appinfo>
                        <meta:metaInfo>
                            <meta:xml>
                                <meta:oldValue>
                                    <meta:sameValue/>
                                    <meta:documentation>Daten nicht authentisch</meta:documentation>
                                </meta:oldValue>
                            </meta:xml>
                        </meta:metaInfo>
                    </xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
                <xs:annotation>
                    <xs:documentation>
                        Temporally invalid
                    </xs:documentation>
                    <xs:appinfo>
                        <meta:metaInfo>
                            <meta:xml>
                                <meta:oldValue>
                                    <meta:sameValue/>
                                    <meta:documentation>Zeitlich ungueltig</meta:documentation>
                                </meta:oldValue>
                            </meta:xml>
                        </meta:metaInfo>
                    </xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="3">
                <xs:annotation>
                    <xs:documentation>
                        Spatially invalid
                    </xs:documentation>
                    <xs:appinfo>
                        <meta:metaInfo>
                            <meta:xml>
                                <meta:oldValue>
                                    <meta:sameValue/>
                                    <meta:documentation>Raeumlich ungueltig</meta:documentation>
                                </meta:oldValue>
                            </meta:xml>
                        </meta:metaInfo>
                    </xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="4">
                <xs:annotation>
                    <xs:documentation>
                        Invalid personal entitlement
                    </xs:documentation>
                    <xs:appinfo>
                        <meta:metaInfo>
                            <meta:xml>
                                <meta:oldValue>
                                    <meta:sameValue/>
                                    <meta:documentation>Persoenlich ungueltig (fuer personengebundene Objekte)</meta:documentation>
                                </meta:oldValue>
                            </meta:xml>
                        </meta:metaInfo>
                    </xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="5">
                <xs:annotation>
                    <xs:documentation>
                        Hotlisted SAM (only for static entitlements)
                    </xs:documentation>
                    <xs:appinfo>
                        <meta:metaInfo>
                            <meta:xml>
                                <meta:oldValue>
                                    <meta:sameValue/>
                                    <meta:documentation>gesperrter SAM (nur Statische Berechtigung)</meta:documentation>
                                </meta:oldValue>
                            </meta:xml>
                        </meta:metaInfo>
                    </xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="6">
                <xs:annotation>
                    <xs:documentation>
                        Hotlisted organisation (only for static entitlements)
                    </xs:documentation>
                    <xs:appinfo>
                        <meta:metaInfo>
                            <meta:xml>
                                <meta:oldValue>
                                    <meta:sameValue/>
                                    <meta:documentation>gesperrte ORG (nur Statische Berechtigung)</meta:documentation>
                                </meta:oldValue>
                            </meta:xml>
                        </meta:metaInfo>
                    </xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="11">
