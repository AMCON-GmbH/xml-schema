                        <meta:since>3.0.0</meta:since>
                    </meta:xml>
                </meta:metaInfo>
            </xs:appinfo>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="entitlementIssuanceParameters" type="eui:IssueEntitlement">
                <xs:annotation>
                    <xs:documentation>Technical parameters to issue the entitlement to the user medium with application.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="entitlementIssuedMetadata" type="po:EntitlementIssuedMetadata" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Entitlement issued metadata to be used in the entitlement issued notification.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
