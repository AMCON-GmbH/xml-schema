    <xs:complexType name="ListVerificationInformation" id="_b9dacac9-c278-44a6-9df4-bbcba61b8336">
        <xs:annotation>
            <xs:documentation>
                Structure which contains information used to verify the consistency and integrity of the
                referenced hotlist or action list.
                May be used after an incremental hotlist / action list was incorporated into a system's inventory
                of hotlist / action list entries to verify that this inventory is consistent with the full list
                that would have been provided by the hotlist service / action management service.
            </xs:documentation>
            <xs:appinfo>
                <meta:metaInfo>
                    <meta:xml>
                        <meta:since>3.0.0</meta:since>
                    </meta:xml>
                </meta:metaInfo>
            </xs:appinfo>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="listCycleNumber" type="tns:ListCycleNumber">
                <xs:annotation>
                    <xs:documentation>
                        Identifies the cycle of the (full) list that the checksum is calculated over.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="checksumOfCorrespondingFullList" type="tns:Checksum">
                <xs:annotation>
                    <xs:documentation>
                        Checksum of the full list.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

